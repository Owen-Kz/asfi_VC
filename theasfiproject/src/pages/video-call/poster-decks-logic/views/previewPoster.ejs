<main>
<!-- =======================
Preview Poster START -->
<section>
	<div class="container">

	    <div class="card-body">
		   <!-- Step 1 content START -->
		    <div class="content">
			    <div class="preview-container">
                   <div class="preview-img" style='overflow-y:scroll; display:flex;'>
					<input type="hidden" id="posterDeckFile" value="<%=PosterFile%>">
					<a href="/uploads/posters/<%=PosterFile%>">
						<div id="pdf-container-1" style="margin: auto; width:100%; height: 100%; display:flex;flex-direction:column; align-items:center;"></div>
					</a>
					
				   </div>

				   <div class="preview-content">
					<h4><%=PosterTitle%></h4>
					<!-- <p>Authors... Lorem ipsum dolor sit, amet consectetur adipisicing elit. Neque nemo eos illum delectus, excepturi consequatur dignissimos tempore perferendis, amet beatae aliquid doloremque ad inventore eveniet facilis aspernatur repellendus est minus?</p> -->

					<h5>Abstract</h5>
					<p><%=Description%></p>

					<span>
						<h5>Presented By</h5>
						<p><%=Presenter%>...</p>
					</span>

					<a href="mailto:<%=PresenterEmail%>">
						<button type="button">Chat with Presenter</button>
					</a>

				   </div>
				</div>
	        </div>
	    </div>
	</div>
</section>
<!-- =======================
Preview Poster END -->

</main>
<!-- **************** MAIN CONTENT END **************** -->

